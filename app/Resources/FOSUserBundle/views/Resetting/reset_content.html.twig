<div class="panel-body">
	<div class="panel panel-default">
		<div class="well well-lg">
			<form class="fos_user_resetting_reset fos_user_resetting_request form-horizontal" action="{{ path('fos_user_resetting_reset', {'token': token}) }}" {{ form_enctype(form) }} method="POST" >
				{{ form_errors(form) }}
    
				<div class="form-group">
					{{ form_label(form.new.first, null, {'label_attr': {'class': 'col-sm-3 control-label'}}) }}
					{{ form_errors(form.new.first) }}
					<div class="col-sm-9">
						{{ form_widget(form.new.first, {'attr': {'class': 'form-control'}}) }}
					</div>
				</div>

				<div class="form-group">
					{{ form_label(form.new.second, null, {'label_attr': {'class': 'col-sm-3 control-label'}}) }}
					{{ form_errors(form.new.second) }}
					<div class="col-sm-9">
						{{ form_widget(form.new.second, {'attr': {'class': 'form-control'}}) }}
					</div>
				</div>

				{{ form_rest(form) }}

				{#{ form_widget(form) }#}

				<div class="form-group">
					<div class="col-sm-offset-3 col-sm-9">
						<button type="submit" class="btn btn-default">
							{{ 'resetting.reset.submit'|trans({}, 'FOSUserBundle') }}
						</button>
					</div>
				</div>
				<!--div class="ym-fbox-button">
					<input class="ym-button" type="submit" value="{{ 'resetting.reset.submit'|trans({}, 'FOSUserBundle') }}" />
				</div-->
			 </form>
		</div>
	</div>
</div>